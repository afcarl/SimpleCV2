[run]
source = simplecv

[report]
omit =
    */IPython/*
    */pexpect/*
    */PIL/*
    */pygame/*
    */python?.?/*
    */site-packages/nose/*
    /usr/share/pyshared/nose/*

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
